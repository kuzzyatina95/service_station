@model IEnumerable<service_station.Models.Car>


@foreach (var car in Model)
            {
    <div class="row">
        <div class="col-xs-10">
            <div class="row">
                <div class="col-xs-3">
                    @Html.DisplayNameFor(model => model.Make):
                </div>
                <div class="col-xs-9">
                    @Html.DisplayFor(model => car.Make)
                </div>
            </div>
            <div class="row">
                <div class="col-xs-3">
                    @Html.DisplayNameFor(model => model.Model):
                </div>
                <div class="col-xs-9">
                    @Html.DisplayFor(model => car.Model)
                </div>
            </div>
            <div class="row">
                <div class="col-xs-3">
                    @Html.DisplayNameFor(model => model.VIN):
                </div>
                <div class="col-xs-9">
                    @Html.DisplayFor(model => car.VIN)
                </div>
            </div>
            <div class="row">
                <div class="col-xs-3">
                    @Html.DisplayNameFor(model => model.Year):
                </div>
                <div class="col-xs-9">
                    @Html.DisplayFor(model => car.Year)
                </div>
            </div>
        </div>
        <div class="col-xs-2">
            <a href="@Url.Action("Edit", "Garage", new { id = car.Id })" class="btn btn-warning btn-circle"><i class="glyphicon glyphicon-pencil"></i></a>
            <br />
            <a href="@Url.Action("Add", "Orders", new {id = car.Id})" class="btn btn-warning btn-circle"><i class="glyphicon glyphicon-plus"></i></a>
            <br />
            @Html.Action("DeleteWithoutOrder", "Garage", new { id = car.Id })
      
        </div>
    </div>
    <hr />
}
@if (Model.Count() == 0)
{
    <p>This client doesn't have cars</p>
}


